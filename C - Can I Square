#include <iostream>
#include <cmath>
using namespace std;
#define ll long long

int main(){
    int t; cin >> t;
    while(t--){
        ll n,count=0; cin >> n;
        for(int i=0;i<n;i++){
            ll aux; cin >> aux;
            count+=aux;
        }
        double sol = sqrt(count);
        if(sol-(int)sol>0)cout << "NO" << endl;
        else cout << "YES" << endl;
    }
}
